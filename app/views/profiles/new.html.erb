<%= form_for @profile, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>

<div>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>
</div>
<div> 
  <%= f.label :country %>
  <%= f.text_field :country %>
</div>
<div>
  <%= f.label :province %>
  <%= f.text_field :province %>
</div>
<div>
  <%= f.label :city %>
  <%= f.text_field :city %>
</div>
  <%= f.submit "Create" %>
<% end %>